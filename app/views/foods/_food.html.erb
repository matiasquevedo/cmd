<tr>
  <td><%= link_to food.name, food %></td>
  <td><%= food.price %></td>
  <td><%= food.menu.name %></td>  
  <td><%= link_to link_badge('warning','fa-edit'),edit_food_path(food) %></td>
  <td><%= link_to link_badge('danger','fa-trash-alt'), food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>

